DROP TABLE TBL_USER;
DROP SEQUENCE SEQ_USER;

CREATE SEQUENCE SEQ_USER;

CREATE TABLE TBL_USER(
	USER_ID NUMBER,
	LOGIN_ID VARCHAR2(1000),
	PASSWORD VARCHAR2(1000),
	NAME VARCHAR2(1000),
	GENDER CHAR(1),
	CONSTRAINTS PK_USER PRIMARY KEY(USER_ID) 
);

INSERT INTO TBL_USER
(USER_ID, LOGIN_ID, PASSWORD, NAME, GENDER)
VALUES(0, '', '', '', '');
